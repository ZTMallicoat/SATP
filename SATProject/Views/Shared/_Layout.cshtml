<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>uiCookies:Enlight &mdash; Free Bootstrap Theme, Free Responsive Bootstrap Website Template</title>
    <meta name="description" content="Free Bootstrap Theme by uicookies.com">
    <meta name="keywords" content="free website templates, free bootstrap themes, free template, free bootstrap, free website template">

    <link href="https://fonts.googleapis.com/css?family=Raleway:300,400,500,700|Open+Sans" rel="stylesheet">
    <link rel="stylesheet" href="~/Content/css/styles-merged.css">
    <link rel="stylesheet" href="~/Content/css/style.min.css">
    <link rel="stylesheet" href="~/Content/css/custom.css">
    <!--[if lt IE 9]>
      <script src="js/vendor/html5shiv.min.js"></script>
      <script src="js/vendor/respond.min.js"></script>
    <![endif]-->
</head>
<body>
    <div class="probootstrap-search" id="probootstrap-search">
        <a href="#" class="probootstrap-close js-probootstrap-close"><i class="icon-cross"></i></a>
        <form action="#">
            <input type="search" name="s" id="search" placeholder="Search a keyword and hit enter...">
        </form>
    </div>

    <div class="probootstrap-page-wrapper">
        <!-- Fixed navbar -->

        <div class="probootstrap-header-top">
            <div class="container">
                <div class="row">
                    <div class="col-lg-9 col-md-9 col-sm-9 probootstrap-top-quick-contact-info">
                        <span><i class="icon-location2"></i>1108 E 10th St, Kansas City, MO 64106</span>
                        <span><i class="icon-phone2"></i>+1 (913)-420-2319</span>
                        <span><i class="icon-mail"></i>Giveus@Money.net</span>
                    </div>
                    <div class="col-lg-3 col-md-3 col-sm-3 probootstrap-top-social">
                        <ul>
                            <li><a href="https://twitter.com/educ_sportsug?lang=en"><i class="icon-twitter"></i></a></li>
                            <li><a href="https://www.facebook.com/EducSportsUg/"><i class="icon-facebook2"></i></a></li>
                            <li><a href="https://www.instagram.com/educsportsug/?hl=en"><i class="icon-instagram2"></i></a></li>
                            <li><a href="https://www.youtube.com/channel/UCNEvA5GEeJ9KuynQWHtcgXw"><i class="icon-youtube"></i></a></li>
                            @*<li><a href="#" class="probootstrap-search-icon js-probootstrap-search"><i class="icon-search"></i></a></li>*@
                        </ul>
                    </div>
                </div>
            </div>
        </div>
        <nav class="navbar navbar-default probootstrap-navbar">
            <div class="container">
                <div class="navbar-header">
                    <div class="btn-more js-btn-more visible-xs">
                        <a href="#"><i class="icon-dots-three-vertical "></i></a>
                    </div>
                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar-collapse" aria-expanded="false" aria-controls="navbar">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <img class="navbar-brand" src="~/Content/img/logo.jpeg" alt="Back Row" style="width:55px;height:55px;margin-bottom:25px;" />
                </div>
                <div id="navbar-collapse" class="navbar-collapse collapse">
                    <ul class="nav navbar-nav navbar-right">
                        <li>@Html.ActionLink("Home", "Index", "Home")</li>
                        <li>@Html.ActionLink("Courses", "Index", "Cours")</li>
                        <li>@Html.ActionLink("Our Mission", "About", "Home")</li>
                        <li>@Html.ActionLink("Contact Us", "Contact", "Home")</li>
                        <!--TODO: Only Teachers (2nd tier admin) have access to these tab-->
                        @if ((User.IsInRole("Admin")||User.IsInRole("Scheduling"))&&Request.IsAuthenticated)
                        {
                            <li>@Html.ActionLink("Students", "Index", "Students")</li>
                            <li>@Html.ActionLink("Enrollments", "Index", "Enrollments")</li>
                        }
                        @Html.Partial("_LoginPartial")
                    </ul>
                </div>
            </div>
        </nav>
        @RenderBody();
        <footer class="probootstrap-footer probootstrap-bg">
            <div class="container">
                <div class="row">
                    <div class="col-md-4">
                        <div class="probootstrap-footer-widget">
                            <h3>About The School</h3>
                            <p>We at B.R.S. Tech are committed to being your rock in these trying times.  We will teach you about stuff and you will learn that stuff usually.  Come to our school, you'll not be a fool!</p>
                            <h3>Social</h3>
                            <ul class="probootstrap-footer-social">
                                @*<li><a href="https://twitter.com/educ_sportsug?lang=en"><i class="icon-twitter"></i></a></li>
                                <li><a href="https://www.facebook.com/EducSportsUg/"><i class="icon-facebook2"></i></a></li>
                                <li><a href="https://www.instagram.com/educsportsug/?hl=en"><i class="icon-instagram2"></i></a></li>
                                <li><a href="https://www.youtube.com/channel/UCNEvA5GEeJ9KuynQWHtcgXw"><i class="icon-youtube"></i></a></li>*@
                                <li><a href="https://twitter.com/educ_sportsug?lang=en"><i class="icon-twitter"></i></a></li>
                                <li><a href="https://www.facebook.com/EducSportsUg/"><i class="icon-facebook"></i></a></li>
                                <li><a href="https://www.instagram.com/educsportsug/?hl=en"><i class="icon-instagram2"></i></a></li>
                                <li><a href="https://www.youtube.com/channel/UCNEvA5GEeJ9KuynQWHtcgXw"><i class="icon-youtube"></i></a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-md-3 col-md-push-1">
                        <div class="probootstrap-footer-widget">
                            <h3>Links</h3>
                            <ul>
                                <li>@Html.ActionLink("Home","Index","Home")</li>
                                <li>@Html.ActionLink("Courses","Index","Cours")</li>
                                <li>@Html.ActionLink("About Us", "About", "Home")</li>
                                <li>@Html.ActionLink("Contact", "Contact", "Home")</li>                           
                            </ul>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="probootstrap-footer-widget">
                            <h3>Contact Info</h3>
                            <ul class="probootstrap-contact-info">
                                <li><i class="icon-location2"></i> <span>1108 E 10th St, Kansas City, MO 64106</span></li>
                                <li><i class="icon-mail"></i><span>Giveus@Money.net</span></li>
                                <li><i class="icon-phone2"></i><span>+1 (913)-420-2319</span></li>
                            </ul>
                        </div>
                        @*<div class="col-lg-9 col-md-9 col-sm-9 probootstrap-top-quick-contact-info">
                            <span><i class="icon-location2"></i>1108 E 10th St, Kansas City, MO 64106</span>
                            <span><i class="icon-phone2"></i>+1 (913)-420-2319</span>
                            <span><i class="icon-mail"></i>Giveus@Money.net</span>
                        </div>*@
                    </div>

                </div>
                <!-- END row -->
            </div>
        </footer>
    </div>
    <!-- END wrapper -->

    <script src="~/Content/js/scripts.min.js"></script>
    <script src="~/Content/js/main.min.js"></script>
    <script src="~/Content/js/custom.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
